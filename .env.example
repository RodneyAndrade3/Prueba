# Configuración de Instagram Scraper
# Copia este archivo como .env y configura tus credenciales

# Credenciales de Instagram
IG_USERNAME=tu_usuario_de_instagram
IG_PASSWORD=tu_contraseña_de_instagram

# Cuenta objetivo para análisis
TARGET_ACCOUNT=cuenta_objetivo

# Tipo de página (followers o following)
PAGE_TYPE=following

# Número de seguidores a analizar
FOLLOWER_COUNT=100

# Número máximo de workers para análisis paralelo
MAX_WORKERS=10
